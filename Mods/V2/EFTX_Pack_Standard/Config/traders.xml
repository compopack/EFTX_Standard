<configs>
	<set xpath="//traders/@currency_item">casinoCoin</set>
	<!-- *** New Traderstage For T4 and T5 -->
	<insertAfter xpath="//traderstage_template[@name='baseTier3']">
		<traderstage_template name="baseTier4">
			<entry min="1" max="999999" quality="0"/>
			<entry min="150" max="999999" quality="1"/>
			<entry min="150" max="999999" quality="2"/>
			<entry min="150" max="999999" quality="3"/>
			<entry min="150" max="999999" quality="4"/>
			<entry min="150" max="999999" quality="5"/>
		</traderstage_template>
		<traderstage_template name="modsTier4">
			<entry min="150" max="999999" quality="0"/>
		</traderstage_template>
		
		<traderstage_template name="baseTier5">
			<entry min="1" max="999999" quality="0"/>
			<entry min="200" max="999999" quality="1"/>
			<entry min="200" max="999999" quality="2"/>
			<entry min="200" max="999999" quality="3"/>
			<entry min="200" max="999999" quality="4"/>
			<entry min="200" max="999999" quality="5"/>
		</traderstage_template>
		<traderstage_template name="modsTier5">
			<entry min="200" max="999999" quality="0"/>
		</traderstage_template>
		
	</insertAfter>		
				
		<append xpath="//trader_item_group[@name='generalResources']">
			<item name="resourceWeaponRepairKit" count="1,5"/>			
			<item name="resourceWeaponParts" count="1,4"/>
			<item name="resourceWD40" count="1,2"/>
			<item name="resourceMultitool" count="1"/>
			<item name="resourceToolset" count="1"/>
		</append>
		<append xpath="//trader_item_group[@name='skillMagazines']">
			<item name="SMGSSkillMagazine" count="1,2"/>			
			<item name="PistolsSkillMagazine" count="1,2"/>							
			<item name="ShotgunsSkillMagazine" count="1,2"/>
			<item name="RocketsandLaunchersSkillMagazine" count="1,2"/>			
			<item name="RiflesSkillMagazine" count="1,2"/>
			<item name="weaponModifiersScopesSkillMagazine" count="1,2"/>
			<item name="weaponModifiersBipodsForegripsSkillMagazine" count="1,2"/>
			<item name="weaponModifiersMagazinesSkillMagazine" count="1,2"/>
			<item name="weaponModifiersSilencersSkillMagazine" count="1,2"/>
			<item name="weaponModifiersStocksSkillMagazine" count="1,2"/>
			<item name="weaponModifiersGrenadeLauncherAttachmentsSkillMagazine" count="1,2"/>
			<item name="HeadLightsandNVGSSkillMagazine" count="1"/>
		</append>	
	
</configs>